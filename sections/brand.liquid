
<section class="bg-black py-8">
    <div class="max-w-[1240px] mx-auto px-6 md:px-0">
        <div class="flex flex-wrap justify-center md:justify-between items-center gap-6">
            {% assign brand_images = "brand1.png,brand2.png,brand3.png,brand4.png,brand5.png" | split: "," %}
            
            {% for brand in brand_images %}
                <img src="{{ brand | asset_url }}" alt="Brand Logo" class="h-6">
            {% endfor %}
        </div>
    </div>
</section>
